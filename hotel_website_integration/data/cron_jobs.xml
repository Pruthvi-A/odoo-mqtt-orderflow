<odoo>
  <data noupdate="1">
    <record id="cron_release_expired_bookings" model="ir.cron">
      <field name="name">Release expired hotel bookings</field>
      <field name="model_id" ref="hotel_management_odoo.model_room_booking"/>
      <field name="state">code</field>
      <field name="code">model._cron_release_expired_bookings() if hasattr(model, '_cron_release_expired_bookings') else None</field>
      <field name="interval_number">1</field>
      <field name="interval_type">days</field>
      <field name="numbercall">-1</field>
      <field name="active">True</field>
    </record>
  </data>
</odoo>